            </picklistValues>
            <picklistValues>
                <fullName>pdf</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>jpg</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>htm</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>html</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>txt</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>htx</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>mp3</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>mpe</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>mpg</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>rtf</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>swf</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
